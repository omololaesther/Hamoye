        "#Q20 & Q21\n",
        "data2 = data.drop(columns = [\"date\",\"lights\"])\n",
        "\n",
        "x_data = data2.drop(columns=[\"Appliances\"])\n",
        "y_data = data2[\"Appliances\"]\n",
        "\n",
        "from sklearn.model_selection import train_test_split\n",
        "x_train,x_test,y_train,y_test = train_test_split(x_data,y_data,test_size = 0.3,random_state = 42)\n",
        "\n",
        "#We will normalize the data\n",
        "from sklearn.preprocessing import StandardScaler\n",
        "Scaler = StandardScaler()\n",
        "x_test_scaled = Scaler.fit_transform(x_test)\n",
        "\n",
        "from sklearn.linear_model import LinearRegression\n",
        "model = LinearRegression()\n",
        "model.fit(x_test_scaled,y_test)\n",
        "test_pred_values = model.predict(x_test_scaled)\n",
        "\n",
        "\n",
        "from sklearn.metrics import mean_absolute_error, mean_squared_error\n",
        "Tested_MAE = mean_absolute_error(y_test,test_pred_values)\n",
        "Tested_RMSE = mean_squared_error(y_test, test_pred_values, squared=False)\n",
        "\n",
        "print(\"Trained Mean Absolute Error:\", round(Tested_MAE,3))\n",
        "print(\"Trained Root Mean Squared Error:\", round(Tested_RMSE,3))"
